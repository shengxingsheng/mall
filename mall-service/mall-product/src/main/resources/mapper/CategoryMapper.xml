<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mall.product.mapper.CategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.mall.product.entity.Category">
        <id column="cat_id" property="catId" />
        <result column="name" property="name" />
        <result column="parent_cid" property="parentCid" />
        <result column="cat_level" property="catLevel" />
        <result column="show_status" property="showStatus" />
        <result column="sort" property="sort" />
        <result column="icon" property="icon" />
        <result column="product_unit" property="productUnit" />
        <result column="product_count" property="productCount" />
        <result column="is_deleted" property="deleted" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        cat_id, name, parent_cid, cat_level, show_status, sort, icon, product_unit, product_count, is_deleted, create_by, create_time, update_by, update_time
    </sql>

</mapper>
